# @name ProductCreate
POST {{Host}}/api/products/create
Content-Type: application/json
Authorization: Bearer {{UserCreate.response.body.token}}

{
  "name": "iPhone 15 Pro",
  "price": 39900
}

###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - Update
