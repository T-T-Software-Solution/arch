@Host= http://localhost:5105

###========================================================= AuditLog
###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - List
GET {{Host}}/api/auditlogs



###============================================================= User
###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - List
GET {{Host}}/api/users/list?pageNo=1&pageSize=3&keyword=n

###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - Create
# @name UserCreate
POST {{Host}}/api/users/create
Content-Type: application/json

{
  "firstName": "John",
  "lastName": "Doe"
}

###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - Update
PUT {{Host}}/api/users/update/{{UserCreate.response.body.userInfo.id}}
Content-Type: application/json
Authorization: Bearer {{UserCreate.response.body.token}}

{
  "firstName": "Mina",
  "lastName": "Dixon"
}

###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - Delete
DELETE {{Host}}/api/users/delete/{{UserCreate.response.body.userInfo.id}}
Authorization: Bearer {{UserCreate.response.body.token}}




###============================================================= Cart
###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - List
GET {{Host}}/api/carts/list

###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - Create
# @name CartCreate
POST {{Host}}/api/carts/create
Authorization: Bearer {{UserCreate.response.body.token}}

###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - Delete
DELETE {{Host}}/api/carts/delete/{{CartCreate.response.body.id}}
Authorization: Bearer {{UserCreate.response.body.token}}



###========================================================== Product
###- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - List
